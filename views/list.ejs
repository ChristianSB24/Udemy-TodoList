<%- include("header") -%>
        <div class="box" id="heading">
            <h1><%= kindOfDay %> </h1>
        </div>

        <div class="box">
                <% for (let item in newTodo) { %>
                    <div class="item">
                        <input type="checkbox">
                        <p><%= newTodo[item] %></p>
                    </div>
            <%    }  %>

            <form class="item" action="/" method="post">
                <input type="text" name="newTodo" placeholder="New Todo" autocomplete="off">
                <button type="submit" name="button" value=<%= typeOfList %>>+</button>
            </form>
        </div>
        
        <script src="" async defer></script>

<%- include("footer") -%>